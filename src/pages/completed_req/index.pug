aside.right-side
    section.content-header
        h1 Completed Requests
        ol.breadcrumb
            li
                router-link(to='/')
                    i.fa.fa-dashboard
                    |  Dashboard
            li Deliveries
            li Completed Requests
    section.content
        .row
            .col-xs-12
                .box
                    .box-header
                        ul.nav.nav-tabs(role='tablist')
                            li.active(role='tablist')
                                a(href='#today' aria-controls='home' role='tab' data-toggle='tab') Today
                            li(role='tablist')
                                a(href='#week' aria-controls='home' role='tab' data-toggle='tab') Week
                            li(role='tablist')
                                a(href='#all' aria-controls='home' role='tab' data-toggle='tab') All
                    .box-body
                        div.tab-content
                            div.tab-pane.active#today(role='tabpanel')
                                div.table-responsive
                                    div.text-center(v-if='dataLoad')
                                        i.fa.fa-refresh.fa-spin.fa-3x.fa-fw
                                    h3.text-center(style='margin: 15px 0;' v-if='!dataLoad && today.length === 0')
                                        | No Data Found!
                                    template(v-if='!dataLoad && today.length > 0')
                                        .row
                                            .col-md-3.col-md-offset-9
                                                .form-group
                                                    input.form-control(type='text' placeholder='Search' v-model='search_table1')
                                        table.table.table-hover(ref='table1')
                                            thead
                                                tr
                                                    th S.No#
                                                    th Date
                                                    th Origin
                                                    th Destination
                                                    th Distance
                                                    th Duration
                                                    //th Parcel
                                                    th Vehicle Type
                                            tbody
                                                tr(v-for='(row, ind) in today')
                                                    td {{ ind+1 }}
                                                    td {{ row.createdAt }}
                                                    td {{ row.orgText }}
                                                    td {{ row.desText }}
                                                    td {{ row.disText }}
                                                    td {{ row.durText }}
                                                    td {{ row.vecType }}
                            div.tab-pane#week(role='tabpanel')
                                div.table-responsive
                                    div.text-center(v-if='dataLoad')
                                        i.fa.fa-refresh.fa-spin.fa-3x.fa-fw
                                    h3.text-center(style='margin: 15px 0;' v-if='!dataLoad && week.length === 0')
                                        | No Data Found!
                                    template(v-if='!dataLoad && week.length > 0')
                                        .row
                                            .col-md-3.col-md-offset-9
                                                .form-group
                                                    input.form-control(type='text' placeholder='Search' v-model='search_table2')
                                        table.table.table-hover(ref='table2')
                                            thead
                                                tr
                                                    th S.No#
                                                    th Date
                                                    th Origin
                                                    th Destination
                                                    th Distance
                                                    th Duration
                                                    //th Parcel
                                                    th Vehicle Type
                                            tbody
                                                tr(v-for='(row, ind) in week')
                                                    td {{ ind+1 }}
                                                    td {{ row.createdAt }}
                                                    td {{ row.orgText }}
                                                    td {{ row.desText }}
                                                    td {{ row.disText }}
                                                    td {{ row.durText }}
                                                    td {{ row.vecType }}
                                span.o_pager_value.center-block.pagination-centered.text-center {{counter}}
                                    |  /
                                    span.o_pager_limit(style="padding-right: 6px;") 1
                                    span.btn-group.btn-group-sm
                                        button.fa.fa-chevron-left.btn.btn-icon.o_pager_previous(v-if='isPrevAvaliable',v-on:click="btnPrev",@click="counter--",style='background-color:white;border-color: #adadad;color: #4c4c4c;')(type='button', accesskey='p')
                                        button.fa.fa-chevron-right.btn.btn-icon.o_pager_next(v-if='isNextAvaliable', v-on:click="btnNext",@click="counter++", style='background-color:white;border-color: #adadad;color: #4c4c4c;')(type='button', accesskey='n')
                            div.tab-pane#all(role='tabpanel')
                                div.table-responsive
                                    div.text-center(v-if='dataLoad')
                                        i.fa.fa-refresh.fa-spin.fa-3x.fa-fw
                                    h3.text-center(style='margin: 15px 0;' v-if='!dataLoad && all.length === 0')
                                        | No Data Found!
                                    template(v-if='!dataLoad && all.length > 0')
                                        .row
                                            .col-md-3.col-md-offset-9
                                                .form-group
                                                    input.form-control(type='text' placeholder='Search' v-model='search_table3')
                                        table.table.table-hover(ref='table3')
                                            thead
                                                tr
                                                    th S.No#
                                                    th Date
                                                    th Origin
                                                    th Destination
                                                    th Distance
                                                    th Duration
                                                    th Vehicle Type
                                            tbody
                                                tr(v-for='(row, ind) in dataToShow')
                                                    td {{ ind+1 }}
                                                    td {{ row.createdAt }}
                                                    td {{ row.orgText }}
                                                    td {{ row.desText }}
                                                    td {{ row.disText }}
                                                    td {{ row.durText }}
                                                    td {{ row.vecType }}
                                span.o_pager_value.center-block.pagination-centered.text-center {{counter}}
                                    |  /
                                    span.o_pager_limit(style="padding-right: 6px;") 2
                                    span.btn-group.btn-group-sm
                                        button.fa.fa-chevron-left.btn.btn-icon.o_pager_previous(v-if='isPrevAvaliable',v-on:click="btnPrev",@click="counter--",style='background-color:white;border-color: #adadad;color: #4c4c4c;')(type='button', accesskey='p')
                                        button.fa.fa-chevron-right.btn.btn-icon.o_pager_next(v-if='isNextAvaliable', v-on:click="btnNext",@click="counter++", style='background-color:white;border-color: #adadad;color: #4c4c4c;')(type='button', accesskey='n')